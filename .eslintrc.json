{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb"
    ],
    "rules": {
        "guard-for-in": 0,
        "no-restricted-syntax":0,
        "no-console":0,
        "array-callback-return":1,
        "class-methods-use-this":0,
        "no-unused-vars":1
    }
}